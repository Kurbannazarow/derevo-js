{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi":"off",
    "linebreak-style":"off",
    "arrow-parens":"off",
    "keyword-spacing":"off",
    "comma-dangle": "off",
    "require-jsdoc": "off"
  },
  "extends": [
    "eslint:recommended", "google"
  ]
}